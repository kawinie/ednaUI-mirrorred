{
    "git": {
        "tagAnnotation": "Release v${version}",
        "commitMessage": "Release v${version}"
    },
    "github": {
        "release": true,
        "releaseName": "Release v${version}"
    },
    "npm": {
        "publish": false
    },
    "plugins": {
        "@release-it/keep-a-changelog": {
            "filename": "CHANGELOG.md",
            "strictLatest": false,
            "keepUnreleased": true
        }
    }
}
